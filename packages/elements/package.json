	"dependencies": {
		"@stencil/core": "latest",
		"@stencil/react-output-target": "latest"
	},
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@pxtrn/storybook-addon-docs-stencil": "latest",
		"@storybook/addon-a11y": "latest",
		"@storybook/addon-actions": "latest",
		"@storybook/addon-essentials": "latest",
		"@storybook/addon-interactions": "latest",
		"@storybook/addon-links": "latest",
		"@storybook/addon-storysource": "latest",
		"@storybook/addon-toolbars": "latest",
		"@storybook/builder-vite": "latest",
		"@storybook/html": "latest",
		"@storybook/react": "latest",
		"@storybook/testing-library": "latest",
		"@storybook/web-components": "latest",
		"@types/jest": "^27.0.3",
		"babel-loader": "^8.2.5",
		"jest": "^27.4.5",
		"jest-cli": "^27.4.5"
	},
	"overrides": {
		"jest-mock": "^28.1.0"
	},
	"license": "MIT"
